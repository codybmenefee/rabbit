# Frontend Environment Configuration
# Copy this file to .env.local and configure the values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000

# Application Configuration
NEXT_PUBLIC_APP_NAME="Rabbit Analytics"
NEXT_PUBLIC_APP_VERSION="2.0.0"

# Feature Flags
NEXT_PUBLIC_ENABLE_API_ENRICHMENT=true
NEXT_PUBLIC_ENABLE_EXPORT=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true

# Logging Configuration
NEXT_PUBLIC_LOG_LEVEL=info
NEXT_PUBLIC_ENABLE_CONSOLE_LOGGING=true
NEXT_PUBLIC_ENABLE_REMOTE_LOGGING=false
NEXT_PUBLIC_LOG_API_CALLS=true
NEXT_PUBLIC_LOG_USER_INTERACTIONS=false
NEXT_PUBLIC_LOG_PERFORMANCE=true
NEXT_PUBLIC_ENABLE_ERROR_BOUNDARY_LOGGING=true

# Analytics (Optional)
NEXT_PUBLIC_GA_TRACKING_ID=
NEXT_PUBLIC_SENTRY_DSN=

# Remote Logging Services (Optional)
NEXT_PUBLIC_LOGTAIL_SOURCE_TOKEN=
NEXT_PUBLIC_DATADOG_CLIENT_TOKEN=
NEXT_PUBLIC_DATADOG_APPLICATION_ID=

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_ENABLE_DEBUG_LOGGING=false 