# Gemma 3 4B Instruct Configuration Example
# Copy this to .env and update with your actual API keys

# LLM Scraping Configuration
LLM_SCRAPING_ENABLED=true
LLM_PROVIDER=google
LLM_MODEL=gemma-3-4b-it

# OpenRouter API Configuration (required for Gemma 3 4B Instruct)
OPENROUTER_API_KEY=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
OPENROUTER_REFERER=https://rabbit-analytics.com
OPENROUTER_TITLE=Rabbit YouTube Analytics

# LLM Performance Settings
LLM_MAX_TOKENS=2000
LLM_TEMPERATURE=0.1
LLM_MAX_CONCURRENT_REQUESTS=5
LLM_REQUEST_DELAY_MS=1000
LLM_RETRY_ATTEMPTS=2
LLM_TIMEOUT_MS=30000
LLM_BATCH_SIZE=10
LLM_COST_LIMIT=10.0

# Caching and Optimization
LLM_CACHE_ENABLED=true
LLM_CACHE_TTL=7200
LLM_HTML_CHUNK_SIZE=50000
LLM_ENABLE_FALLBACK=true

# Alternative Models (fallback options)
# LLM_MODEL=gemma-3n-e4b-it  # Free version
# LLM_MODEL=claude-3-haiku-20240307  # Higher quality, higher cost
# LLM_MODEL=gpt-3.5-turbo  # OpenAI alternative

# Other Services (optional)
YOUTUBE_API_KEY=your_youtube_api_key_here
MONGODB_URI=mongodb://localhost:27017/youtube-analytics

# Server Configuration
PORT=5000
NODE_ENV=development 