{
  "name": "rabbit-backend",
  "version": "2.0.0",
  "description": "Rabbit YouTube Analytics Platform - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/backend/unit",
    "test:integration": "jest tests/backend/integration",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "youtube",
    "analytics",
    "watch-history",
    "business-intelligence",
    "data-visualization"
  ],
  "author": "Rabbit Analytics Team",
  "license": "MIT",
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "axios": "^1.6.2",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "fast-html-parser": "^1.0.1",
    "googleapis": "^129.0.0",
    "http2-wrapper": "^2.2.1",
    "jsdom": "^23.0.1",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "p-limit": "^3.1.0",
    "piscina": "^4.9.2",
    "playwright": "^1.54.1",
    "redis": "^4.6.11",
    "tough-cookie": "^4.1.4",
    "typescript": "^5.3.3",
    "undici": "^6.21.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/backend/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.test.ts",
      "!src/index.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../tests/backend/helpers/setup.ts"
    ]
  }
}
