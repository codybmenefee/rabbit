{
  "name": "rabbit",
  "version": "1.0.0",
  "description": "A data analytics platform for media consumption insights across multiple platforms",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "platforms/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspace=apps/web",
    "start": "npm run start --workspace=apps/web",
    "lint": "npm run lint --workspace=apps/web",
    "test": "npm run test --workspace=apps/web",
    "type-check": "npm run type-check --workspace=apps/web",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules platforms/*/node_modules",
    "install:all": "npm install && npm run install:all --workspace=apps/web",
    "validate:all": "npm run validate:all --workspace=apps/web"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/rabbit.git"
  },
  "keywords": [
    "analytics",
    "data",
    "media",
    "youtube",
    "insights",
    "consumption",
    "platform"
  ],
  "author": "Rabbit Team",
  "license": "MIT"
}
